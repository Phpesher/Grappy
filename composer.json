{
  "name": "lamantin/grappy",
  "description": "Component-based MVC framework",
  "type": "framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Nikita",
      "email": "superduperproger@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "twig/twig": "1.*",
    "vlucas/phpdotenv": "^4.1",
    "monolog/monolog": "^2.0",
    "phroute/phroute": "dev-master",
    "illuminate/database": "5.1.8",
    "robmorgan/phinx": "0.11.x-dev"
  },
  "autoload": {
    "psr-4": {
      "Lamantin\\App\\": "app/"
